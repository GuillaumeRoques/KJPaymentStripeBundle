        "jms/payment-core-bundle": "^1.1",
        "stripe/stripe-php": "dev-master",
        "symfony/framework-bundle": "^2.7",
        "symfony/http-kernel": "^2.7",
        "symfony/form": "^2.7",
        "symfony/dependency-injection": "^2.7",
        "symfony/validator": "^2.7",
        "doctrine/orm": "^2.5"
